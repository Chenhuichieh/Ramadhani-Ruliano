#soal no 1
n= int(input("masukan nilai n :"))
total = 0
for i in range (1, n+1):
    total += i
print (total)

#soal no 2
r= float(input("jari jari lingkaran adalah (r) : "))
luas = 3.14 * r ** 2
print ("Jadi jari jari lingkaran adalah : ", luas)

#soal no 3
for i in range (1, 6):
  print (i)

#soal no 4
for i in range (1, 11, 2):
  print (i)
